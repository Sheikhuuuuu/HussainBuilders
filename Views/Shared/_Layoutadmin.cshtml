<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Admin</title>

    <!-- Vendor CSS -->
    <link href="/Contents/admin/vendors/bower_components/fullcalendar/dist/fullcalendar.min.css" rel="stylesheet">
    <link href="/Contents/admin/vendors/bower_components/animate.css/animate.min.css" rel="stylesheet">
    <link href="/Contents/admin/vendors/bower_components/sweetalert/dist/sweetalert.css" rel="stylesheet">
    <link href="/Contents/admin/vendors/bower_components/material-design-iconic-font/dist/css/material-design-iconic-font.min.css" rel="stylesheet">
    <link href="/Contents/admin/vendors/bower_components/malihu-custom-scrollbar-plugin/jquery.mCustomScrollbar.min.css" rel="stylesheet">

    <!-- CSS -->
    <link href="/Contents/admin/css/app_1.min.css" rel="stylesheet">
    <link href="/Contents/admin/css/app_2.min.css" rel="stylesheet">

</head>
<script type="text/javascript"> window.history.forward(); function noBack() { window.history.forward(); }</script>





<body class="dashboard-page" onload="        noBack();" onpageshow="        if (event.persisted) noBack();" onunload="">
    <header id="header" class="clearfix" data-ma-theme="blue">
        <ul class="h-inner">
            <li class="hi-trigger ma-trigger" data-ma-action="sidebar-open" data-ma-target="#sidebar">
                <div class="line-wrap">
                    <div class="line top"></div>
                    <div class="line center"></div>
                    <div class="line bottom"></div>
                </div>
            </li>

            <li class="hi-logo hidden-xs">
                <a href="/admin/Index">Hussain Buildes & Contractors.</a>
            </li>
        </ul>

        <!-- Top Search Content -->
        <div class="h-search-wrap">
            <div class="hsw-inner">
                <i class="hsw-close zmdi zmdi-arrow-left" data-ma-action="search-close"></i>
                <input type="text">
            </div>
        </div>
    </header>

    <section id="main">
        <aside id="sidebar" class="sidebar c-overflow">
            <div class="s-profile">
                <a href="#" data-ma-action="profile-menu-toggle">
                    <div class="sp-pic">
                        <img src="/Contents/admin/img/profile-pics/profile.jpg" alt="">
                    </div>

                    <div class="sp-info">
                        Hussain Builders

                        <i class="zmdi zmdi-caret-down"></i>
                    </div>
                </a>

                <ul class="main-menu">
                    
                    <li>
                        <a href="~/admin/logout"><i class="zmdi zmdi-time-restore"></i> Logout</a>
                    </li>
                </ul>
            </div>

            <ul class="main-menu">
                <li class="active">
                    <a href="/admin/Index"><i class="zmdi zmdi-home"></i> Home</a>
                </li>

                <li><a href="/admin/Company"><i class="zmdi zmdi-format-underlined"></i> Contact Information</a></li>

                <li><a href="/admin/showclient"><i class="zmdi zmdi-calendar"></i> Show Clients </a></li>

                <li><a href="/admin/showteam"><i class="zmdi zmdi-layers"></i> Show Team</a></li>
                <li>
                    <a href="/admin/showprojects"><i class="zmdi zmdi-money"></i>Show Projects</a>
                </li>
            </ul>
        </aside>

        <aside id="chat" class="sidebar">

            <div class="chat-search">
                <div class="fg-line">
                    <input type="text" class="form-control" placeholder="Search People">
                    <i class="zmdi zmdi-search"></i>
                </div>
            </div>

            <div class="lg-body c-overflow">
                <div class="list-group">
                    <a class="list-group-item media" href="#">
                        <div class="pull-left p-relative">
                            <img class="lgi-img" src="/Contents/admin/img/profile-pics/2.jpg" alt="">
                            <i class="chat-status-busy"></i>
                        </div>
                        <div class="media-body">
                            <div class="lgi-heading">Jonathan Morris</div>
                            <small class="lgi-text">Available</small>
                        </div>
                    </a>

                    <a class="list-group-item media" href="#">
                        <div class="pull-left">
                            <img class="lgi-img" src="/Contents/admin/img/profile-pics/1.jpg" alt="">
                        </div>
                        <div class="media-body">
                            <div class="lgi-heading">David Belle</div>
                            <small class="lgi-text">Last seen 3 hours ago</small>
                        </div>
                    </a>

                    <a class="list-group-item media" href="#">
                        <div class="pull-left p-relative">
                            <img class="lgi-img" src="/Contents/admin/img/profile-pics/3.jpg" alt="">
                            <i class="chat-status-online"></i>
                        </div>
                        <div class="media-body">
                            <div class="lgi-heading">Fredric Mitchell Jr.</div>
                            <small class="lgi-text">Availble</small>
                        </div>
                    </a>

                    <a class="list-group-item media" href="#">
                        <div class="pull-left p-relative">
                            <img class="lgi-img" src="/Contents/admin/img/profile-pics/4.jpg" alt="">
                            <i class="chat-status-online"></i>
                        </div>
                        <div class="media-body">
                            <div class="lgi-heading">Glenn Jecobs</div>
                            <small class="lgi-text">Availble</small>
                        </div>
                    </a>

                    <a class="list-group-item media" href="#">
                        <div class="pull-left">
                            <img class="lgi-img" src="/Contents/admin/img/profile-pics/5.jpg" alt="">
                        </div>
                        <div class="media-body">
                            <div class="lgi-heading">Bill Phillips</div>
                            <small class="lgi-text">Last seen 3 days ago</small>
                        </div>
                    </a>

                    <a class="list-group-item media" href="#">
                        <div class="pull-left">
                            <img class="lgi-img" src="/Contents/admin/img/profile-pics/6.jpg" alt="">
                        </div>
                        <div class="media-body">
                            <div class="lgi-heading">Wendy Mitchell</div>
                            <small class="lgi-text">Last seen 2 minutes ago</small>
                        </div>
                    </a>
                </div>
            </div>

        </aside>

        <section id="content">
            <div class="container">

                <div>

                </div>
                @RenderBody()





            </div>
        </section>
    </section>

    <footer id="footer">
        @*Copyright &copy; 2016 Adnan Rafique*@


    </footer>

    <!-- Page Loader -->
    <div class="page-loader">
        <div class="preloader pls-blue">
            <svg class="pl-circular" viewBox="25 25 50 50">
                <circle class="plc-path" cx="50" cy="50" r="20" />
            </svg>

            <p>Please wait...</p>
        </div>

    </div>


