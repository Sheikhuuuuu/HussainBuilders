    <script src="/Contents/admin/vendors/bower_components/fullcalendar/dist/fullcalendar.min.js"></script>
    <script src="/Contents/admin/vendors/bower_components/simpleWeather/jquery.simpleWeather.min.js"></script>
    <script src="/Contents/admin/vendors/bower_components/Waves/dist/waves.min.js"></script>
    <script src="/Contents/admin/vendors/bootstrap-growl/bootstrap-growl.min.js"></script>
    <script src="/Contents/admin/vendors/bower_components/sweetalert/dist/sweetalert.min.js"></script>
    <script src="/Contents/admin/vendors/bower_components/malihu-custom-scrollbar-plugin/jquery.mCustomScrollbar.concat.min.js"></script>

